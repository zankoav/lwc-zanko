							                                <div class="left">
							                                    <div class="col">
							                                        <label for="Company">
							                                            Ulica*:
							                                        </label>
							                                        <apex:inputText id="shipStreetId" html-placeholder="Proszę wpisać" value="{! shipStreet }"/> 
							                                    </div>  
							                                    <div class="col">
							                                        <label for="Company">
							                                            Miasto*: 
							                                        </label>
							                                        <apex:inputText id="shipCityId" styleClass="text-box single-line" html-placeholder="Proszę wpisać" value="{! shipCity }"/>
