<apex:page controller="AllStarMondoFormController" action="{! init }" sidebar="false" showHeader="false" standardStylesheets="false">
	<apex:form >
		<apex:includeScript value="{!$Resource.jQuery}"/>
			<apex:stylesheet value="{!URLFOR($Resource.AllstarFiles,'css/webtype.css')}"/>
			<apex:stylesheet value="{!URLFOR($Resource.AllstarFiles,'css/font.css')}"/>
			<apex:stylesheet value="{!URLFOR($Resource.AllstarFiles,'css/card_page.css')}"/>
			<apex:stylesheet value="{!URLFOR($Resource.AllstarFiles,'css/DependencyHandler.css')}"/>
			<apex:stylesheet value="{!URLFOR($Resource.AllstarFiles,'css/developers.css')}"/>
			<apex:stylesheet value="{!URLFOR($Resource.AllstarFiles,'css/style.css')}"/>
		<head>
		    <meta name="viewport" content="width=device-width"/>
		    <title>Zamów teraz | All Star Mondo</title>
		  	<script type="text/javascript">
				function clearFieldsBusAddressBlock(checkbox) {
					if (checkbox.checked) {
						document.getElementById('{!$Component.shipStreetId}').value = '';
						document.getElementById('{!$Component.shipCityId}').value = '';
						document.getElementById('{!$Component.shipPostCodeId}').value = '';
						document.getElementById('{!$Component.shipCountryId}').value = '';
					}	
				}
			</script>
			<script type="text/javascript">
				function clearBusAddressBlockFields(checkbox) {
					if (checkbox.checked) {
						document.getElementById('{!$Component.shipStreetId}').value = '';
						document.getElementById('{!$Component.shipCityId}').value = '';
						document.getElementById('{!$Component.shipPostCodeId}').value = '';
						document.getElementById('{!$Component.shipCountryId}').value = '';
					}	
				}				
			</script>			
		</head>
		<body class="node-type-subpage node-template-subpage html not-front not-logged-in no-sidebars page-node page-node- page-node-6 node-type-apply-now">
			<style>		
				#logo .showLogo {
				    height: 83px;
				    width: 202px;
				    background: url("{!URLFOR($Resource.AllstarFiles,'img/alstar_mondo_pl.png')}") 0 0 no-repeat;
				}
				.node-type-apply-now #tabs .tabs li a.completed-step + span.step-number {
				    text-indent: -9999px;
				    white-space: nowrap;
				    width: 26px;
				    height: 26px;
				    background: #fff url("{!URLFOR($Resource.AllstarFiles,'img/step-ok.png')}") 50% 50% no-repeat;
				    border: 2px solid #ffc725;
				    border-radius: 26px;
				    font-family: Antenna RE Bd;
				    font-weight: bold;
				    font-size: 16px;
				    display: inline-block;
				    z-index: 10;
				    position: relative;
				    top: -15px;
				}
				#mega-footer {
				    background: url("{!URLFOR($Resource.AllstarFiles,'img/allstar_carbon_extended.png')}") #000;
				    background-repeat: repeat-y;
				    color: #fff;
				    min-height: 80px;
				    overflow: hidden;
				    padding: 0 50px 50px 10%;
				    position: relative;
				    font-size: 11px;
				    background-size: cover;
				}

				.checkbox {
					height: 18px;
					font-size: 14px;
					display: block;
					position: relative;
					padding: 0;
					margin: 0;
				}
				
				.checkbox input[type='checkbox'] {
					position: absolute;
					left: 0;
				}

				.checkbox span {
				    margin: 0;
				    position: absolute;
				    left: 25px;
				    font-family: Antenna RE Reg;
				    font-size: 16px;
				    font-weight: normal;
				}

				.message.confirmM3 {
					width: 400px;
				    padding: 20px;
				    color: green !important;
				    border: 1px solid green;
				    margin: 0px auto;
				}

				.message.errorM3 {
					width: 307px;
				    padding: 20px;
				    color: red !important;
				    border: 1px solid red;
				    margin: 0px auto;
				}

				.message.errorM3 h4,
				.message.confirmM3 h4  {
					display: none;
				}

				#toStepTwo { background: #000; border-width: 0; color: #fff; cursor: pointer; display: block; float: right; font-size: 16px; font-weight: bold; height: 50px; line-height: 33px; margin: 0 35px 0 0; padding: 0 50px;}

				.node-type-apply-now .headerimage {background: rgba(0, 0, 0, 0) url("{!URLFOR($Resource.AllstarFiles,'img/header-box-without.png')}") no-repeat scroll 0 0 / cover ;  height: 370px; z-index: 1;overflow:hidden;}
				@media screen and (max-width:1500px) {
			  	.node-type-apply-now .headerimage {background: rgba(0, 0, 0, 0) url("{!URLFOR($Resource.AllstarFiles,'img/header-box-without.png')}") no-repeat scroll 0 0 / cover ;  height: 340px; z-index: 1;overflow:hidden;}
				}
				@media screen and (max-width:1200px) {
		  		.node-type-apply-now .headerimage {background: rgba(0, 0, 0, 0) url("{!URLFOR($Resource.AllstarFiles,'img/header-box-without.png')}") no-repeat scroll 0 0 / cover ;  height: 300px; z-index: 1;overflow:hidden;}

			</style>
			<div id="page" class="container" style="align-content: center;">
