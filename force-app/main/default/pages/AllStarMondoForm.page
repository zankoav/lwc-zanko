							                                <div class="right">
							                                    <div class="col">
							                                        <label for="Company">
							                                            Kod pocztowy*: 
							                                        </label>
							                                        <apex:inputText id="shipPostCodeId" styleClass="text-box single-line" html-placeholder="Proszę wpisać" value="{! shipPostCode }"/>
							                                        <span class="field-validation-valid" data-valmsg-for="Company" data-valmsg-replace="true">
							                                        </span>
							                                    </div>
							                                    <div class="col">
							                                        <label for="Company">
							                                            Państwo*: 
							                                        </label>
							                                        <apex:inputText id="shipCountryId" styleClass="text-box single-line" html-placeholder="Proszę wpisać" value="{! shipCountry }"/> 
							                                    </div> 
							                                </div>
							                            </apex:outputPanel>
						                            </apex:outputpanel>   

						                            <div class="row" style="clear:both;float:right">					           
					                                	<apex:commandbutton id="toStepTwo" value="Dalej" action="{! goToStepTwo }" rerender="formByStepOne,formByStepTwo,formByStepThree"/>
